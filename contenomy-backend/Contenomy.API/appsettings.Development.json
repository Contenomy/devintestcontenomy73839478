{
  "AllowedHosts": "*",
  "App": "Contenomy",
  "Auth": {
    "Domain": "localhost"
  },
  "Cors": {
    "AllowedOrigins": [
      "http://localhost:3000"
    ]
  },
  "ConnectionStrings": {
    "DefaultConnection": ""
  },
  "Serilog": {
    "MinimumLevel": {
      "Default": "Debug",
      "Override": {
        "Microsoft": "Debug",
        "System": "Debug"
      }
    },
    "WriteTo": [
      {
        "Name": "Console",
        "Args": {
          "outputTemplate": "[{Timestamp:HH:mm:ss} {Level:u3}] {Message:lj}{NewLine}{Exception}"
        }
      }
    ]
  },
  "Mangopay": {
    "clientId": "",
    "apiKey": "",
    "sandbox": "https://api.sandbox.mangopay.com/v2.01",
    "tokenurl": "/v2.01/oauth/token"
  },
  "InfluxConfig": {
    "Token": "",
    "Server": "http://localhost:8086",
    "Bucket": "contenomy",
    "Org": "contenomy"
  }
}
