{
	"compilerOptions": {
		"target": "es5",
		"lib": ["dom", "dom.iterable", "esnext"],
		"allowJs": true,
		"baseUrl": "./",
		"skipLibCheck": true,
		"strict": true,
		"noEmit": true,
		"esModuleInterop": true,
		"module": "esnext",
		"moduleResolution": "bundler",
		"resolveJsonModule": true,
		"isolatedModules": true,
		"jsx": "preserve",
		"incremental": true,
		"paths": {
			"@components/*": ["./src/components/*"],
			"@environment/*": ["./src/environment/*"],
			"@service/*": ["./src/services/*"],
			"@context/*": ["./src/context/*"],
			"@model/*": ["./src/model/*"],
			"@pages/*": ["./src/pages/*"],
			"@init/*": ["./src/init/*"],
			"@hooks/*": ["./src/hooks/*"]
		}
	},
	"include": [
		"react-app-env.d.ts",
		"**/*.ts",
		"**/*.tsx",
		".next/types/**/*.ts",
		"config-overrides.js",
		"tailwind.config.js"
, "src/app/user/index"	],
	"exclude": ["node_modules"]
}
